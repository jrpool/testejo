{
  "id": "instacartcart",
  "what": "Instacart cart",
  "strict": true,
  "commands": [
    {
      "type": "launch",
      "which": "webkit",
      "what": "Webkit browser"
    },
    {
      "type": "url",
      "which": "https://www.instacart.com/store/cvs/storefront",
      "what": "Instacart cart"
    },
    {
      "type": "button",
      "which": "Add 1 item CVS Pharmacy Rigid Tip",
      "index": 0,
      "what": "product"
    },
    {
      "type": "button",
      "which": "Close modal",
      "what": "Close login dialog"
    },
    {
      "type": "button",
      "which": "View Cart",
      "what": "View-cart button"
    },
    {
      "type": "link",
      "which": "Go to Checkout",
      "what": "Checkout link pretending to be a button"
    },
    {
      "type": "wait",
      "what": "body",
      "which": "temporarily authorized"
    },
    {
      "type": "test",
      "which": "motion",
      "what": "spontaneous change of content",
      "delay": 2400,
      "interval": 2600,
      "count": 5
    },
    {
      "type": "test",
      "which": "axe",
      "withItems": true,
      "rules": [],
      "what": "Axe core, all rules"
    },
    {
      "type": "test",
      "which": "bulk",
      "what": "count of visible elements"
    },
    {
      "type": "test",
      "which": "embAc",
      "what": "active elements incorrectly embedded in each other",
      "withItems": true
    },
    {
      "type": "test",
      "which": "focAll",
      "what": "Tab-focusability"
    },
    {
      "type": "test",
      "which": "focInd",
      "what": "focus indicators",
      "withItems": true,
      "revealAll": true
    },
    {
      "type": "test",
      "which": "focOp",
      "what": "focusability and operability of elements",
      "withItems": true
    },
    {
      "type": "test",
      "which": "hover",
      "what": "hover-triggered content changes",
      "withItems": true
    },
    {
      "type": "test",
      "which": "labClash",
      "what": "unlabeled and mislabeled form controls",
      "withItems": true
    },
    {
      "type": "test",
      "which": "linkUl",
      "what": "underlining of inline links",
      "withItems": true
    },
    {
      "type": "test",
      "which": "menuNav",
      "what": "keyboard navigation within true-focus menus",
      "withItems": true
    },
    {
      "type": "test",
      "which": "radioSet",
      "what": "grouping of radio buttons in fieldsets",
      "withItems": true
    },
    {
      "type": "test",
      "which": "role",
      "what": "validity and necessity of role assignments"
    },
    {
      "type": "test",
      "which": "styleDiff",
      "what": "style consistency of headings, buttons, and links",
      "withItems": true
    },
    {
      "type": "test",
      "which": "tabNav",
      "what": "keyboard navigation within tab lists",
      "withItems": true
    },
    {
      "type": "test",
      "which": "zIndex",
      "what": "elements with non-auto z indexes",
      "withItems": true
    },
    {
      "type": "test",
      "which": "ibm",
      "withNewContent": false,
      "withItems": true,
      "what": "IBM Accessibility Checker"
    },
    {
      "type": "test",
      "which": "aatt",
      "waitLong": true,
      "what": "AATT with HTML CodeSniffer"
    },
    {
      "type": "test",
      "which": "alfa",
      "what": "Siteimprove alfa"
    }
  ]
}
